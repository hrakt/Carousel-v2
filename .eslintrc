{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "plugins": ["babel", "react"],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "globals": {},
    "settings": {
        "react": {
            "createClass": "createReactClass",
            "pragma": "React",
            "version": "detect"
        }
    },
    "rules": {
        "no-empty": 0,
        "react/display-name": 0
    }
}
